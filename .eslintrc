{
    "parser": "babel-eslint",
    "env": {
        "es6":true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "linebreak-style": "off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "indent": ["error", 4],
        "require-jsdoc":"off"
    },
    "extends": [
        "eslint:recommended", "google"
    ]
}